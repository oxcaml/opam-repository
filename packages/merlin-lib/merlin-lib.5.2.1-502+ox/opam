  "csexp"     {>= "1.5.1" & <= "1.5.2"}
  "alcotest"  {with-test & >= "1.3.0"}
  "menhir"    {dev & >= "20201216" & <= "20240715"}
  "menhirLib" {dev & >= "20201216" & <= "20240715"}
  "menhirSdk" {dev & >= "20201216" & <= "20240715"}
]
build: [
  ["dune" "subst"] {dev}
  ["dune" "build" "-p" name "-j" jobs]
]
