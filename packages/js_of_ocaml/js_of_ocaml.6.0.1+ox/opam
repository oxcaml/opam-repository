  "js_of_ocaml-compiler" {= version & = "6.0.1+ox"}
  "ppxlib"               {= "0.33.0+ox"}
  "num"                  {with-test}
  "ppx_expect"           {>= "v0.14.2" & with-test}
  "re"                   {>= "1.9.0" & with-test & = "1.14.0+ox"}
  "odoc"                 {with-doc}
]
